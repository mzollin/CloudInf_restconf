
<ip xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-native"  xmlns:ios="http://cisco.com/ns/yang/Cisco-IOS-XE-native">
{% for route in routes %}
<route>
  <ip-route-interface-forwarding-list>
    <prefix>{{ route.prefix }}</prefix>
    <mask>{{ route.mask }}</mask>
    <fwd-list>
      {% for forward in route.forward_list %}
      <fwd>{{ forward.fwd }}</fwd>
      {% endfor %}
    </fwd-list>
  </ip-route-interface-forwarding-list>
</route>
{% endfor %}
</ip>

